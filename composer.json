{
    "name": "cstudios/turbo",
    "description": "This plugin utilizes the PageCache filter from yii2 into your craft 3 instance",
    "type": "craft-plugin",
    "version": "1.4.2",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "page cache",
        "turbo"
    ],
    "support": {
        "docs": "https://github.com/cstudiossro/turbo/blob/master/README.md",
        "issues": "https://github.com/cstudiossro/turbo/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Gergely Horvath",
            "homepage": "https://cstudios.sk"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "craftcms/cms": "^3.0.0|^4.0"
    },
    "autoload": {
        "psr-4": {
          "cstudios\\turbo\\": "src/"
        }
    },
    "extra": {
        "name": "Turbo",
        "handle": "turbo",
        "developer": "Gergely Horvath",
        "developerUrl": "https://cstudios.sk",
        "documentationUrl": "https://github.com/cstudiossro/turbo/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/cstudiossro/turbo/master/CHANGELOG.md",
        "class": "cstudios\\turbo\\Turbo"
    }
}
